Bundle 'rstacruz/sparkup', {'rtp': 'vim/'}
Bundle 'L9'
Bundle 'FuzzyFinder'
Bundle 'EasyClip'
Bundle 'Lokaltog/vim-powerline'
Bundle 'Align'
